

CREATE COMPUTE MODULE Logger_Route
	DECLARE LOG_LEVEL EXTERNAL INTEGER 0;
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		IF (LOG_LEVEL=1) THEN 
			PROPAGATE TO TERMINAL 'out1' MESSAGE InputRoot;
		END IF;
		IF (LOG_LEVEL=2) THEN 
			PROPAGATE TO TERMINAL 'out2' MESSAGE InputRoot;
		END IF;
		SET OutputRoot = InputRoot;
		RETURN TRUE;
	END;
END MODULE;
